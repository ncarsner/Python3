# script:   Leet translator
# author:   Nicholas Carsner
# created:  2021-10-20
# updated:  

words = [
    'analyst',
    'brother',
    'calculate',
    'daffodil',
    'equality',
    'fidelity',
    'gravity',
    'hopeful',
    'illogical',
    'juliet',
    'kanban',
    'listener',
    'magenta',
    'negative',
    'obfuscate',
    'python',
    'question',
    'resolve',
    'solicit',
    'twilight',
    'uniform',
    'victory',
    'whiskey',
    'xylophone',
    'yankee',
    'zippered'

]

leet = {
    'a':'4',
    'e':'3',
    'i':'1',
    'o':'0',
    's':'5'
}

for word in words:
    for x,y in leet.items():
        word = word.lower().replace(x,y)
    print(word.upper())
